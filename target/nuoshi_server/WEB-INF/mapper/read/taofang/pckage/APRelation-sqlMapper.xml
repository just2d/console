<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nuoshi.console.persistence.read.taofang.pckage.APRelationReadMapper" >

  	<select id="getAgentSpecialPackage" resultType="com.nuoshi.console.domain.pckage.AgentPackageRelation">
    	SELECT 
	    	agent_id agentId,
	    	package_id packageId,
	    	price
    	FROM agent_package_relation WHERE agent_id = #{agentId}
  	</select>
 
</mapper>